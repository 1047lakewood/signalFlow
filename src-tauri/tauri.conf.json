{
  "productName": "signalFlow",
  "version": "0.1.0",
  "identifier": "com.signalflow.app",
  "build": {
    "frontendDist": "../gui/dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": {
      "script": "node scripts/tauri-dev-server.mjs",
      "cwd": "../gui"
    },
    "beforeBuildCommand": {
      "script": "npm run build",
      "cwd": "../gui"
    }
  },
  "app": {
    "windows": [
      {
        "title": "signalFlow",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}
